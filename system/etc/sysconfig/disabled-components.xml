<?xml version="1.0" encoding="utf-8"?>
<config>
    <component-override package="com.samsung.android.smartmirroring">     
        <component class="com.samsung.android.smartmirroring.settings.DisableSecondScreenActivity" enabled="false" />
        <component class="com.samsung.android.smartmirroring.player.SecondScreenActivity" enabled="true" />
        <component class="com.samsung.android.smartmirroring.tile.ScreenSharingTile" enabled="true" />
    </component-override>

    <component-override package="com.samsung.android.smartsuggestions">
        <component class="com.samsung.android.smartsuggestions.widget.appwidget.SmartSuggestionsWidgetProvider" enabled="true" />
    </component-override>
</config>

